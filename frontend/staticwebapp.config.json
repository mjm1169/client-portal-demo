{
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/dashboard.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/hierarchy.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/reports.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/data/*",
        "allowedRoles": ["authenticated"]
      }
    ],

    "navigationFallback": {
      "rewrite": "/dashboard.html",
      "exclude": [
        "/api/*",
        "/*.js",
        "/*.css"
      ]
    }
  }